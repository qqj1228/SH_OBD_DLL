<?xml version="1.0" encoding="utf-8"?>
<OBDResultSetting xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <!-- 无论何种情况都上传数据，即使结果为不合格 -->
  <UploadWhenever>false</UploadWhenever>
  <!-- OBD检测结果是否需要判断“MIL故障灯” -->
  <MIL>false</MIL>
  <!-- OBD检测结果是否需要判断“存在存储DTC” -->
  <DTC03>false</DTC03>
  <!-- OBD检测结果是否需要判断“存在未决DTC” -->
  <DTC07>false</DTC07>
  <!-- OBD检测结果是否需要判断“存在永久DTC” -->
  <DTC0A>false</DTC0A>
  <!-- OBD检测结果是否需要判断“未完成就绪状态超过2项” -->
  <Readiness>false</Readiness>
  <!-- OBD检测结果是否需要判断“扫码VIN号、读取VIN号，这两个VIN号匹配” -->
  <VINError>true</VINError>
  <!-- OBD检测结果判定：国六车型是否允许CAL_ID或者CVN为空值 -->
  <CALIDCVNEmpty>false</CALIDCVNEmpty>
  <!-- OBD检测结果是否需要判断“OBD型式”为空或非标准值 -->
  <OBD_SUP>true</OBD_SUP>
  <!-- 自动上传以前上传失败的数据的时间间隔（单位：分钟） -->
  <UploadInterval>15</UploadInterval>
  <!-- 检测报告编号里的顺序号起始值 -->
  <StartSN>7000</StartSN>
  <!-- CAL_ID含有连续n个或以上的非字母或数字即判定为不合格，n为下面设定的数字 -->
  <UnmeaningNum>5</UnmeaningNum>
  <!-- 检测报告编号中的企业编号 -->
  <CompanyCode>0079</CompanyCode>
</OBDResultSetting>